<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="hookah_sql.tabacco.Musthave" table="musthave">
        <id name="id" column="id" type="int">
            <generator class="identity" />
        </id>
        <property name="name" column="name" not-null="true" type="string" />
        <property name="flavor" column="flavor" not-null="true" type="string" />
        <property name="description" column="description" type="string" />
        <property name="taste" column="taste" not-null="true" type="string" />
        <property name="hardness" column="hardness" not-null="true" type="int" />
    </class>
</hibernate-mapping>